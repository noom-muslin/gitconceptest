                    },
                    "subModel": {
                      "type": "string"
                    },
                    "subModelDesc": {
                      "type": "string"
                    },
                    "vehicleKey": {
                      "type": "string"
                    },
                    "redbookPrice": {
                      "type": "number"
                    },
                    "engineTypeDesc": {
                      "type": "string"
                    },
                    "brandLevel": {
                      "type": "string"
                    },
                    "modelLevel": {
                      "type": "string"
                    },
                    "redbookBrand": {
                      "type": "string"
                    },
                    "redbookModel": {
                      "type": "string"
