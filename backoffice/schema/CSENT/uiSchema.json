{
  "type": "BoxLayout",
  "elements": [
    {
      "type": "BoxLayout",
      "options": {
        "sx": {
          "padding": "16px",
          "backgroundColor": "primary.main"
        }
      },
      "elements": [
        {
          "type": "Typography",
          "label": "CSENT",
          "options": {
            "variant": "h6White",
            "sx": {
              "padding": "4px 0"
            }
          }
        }
      ]
    },
    {
      "type": "StackLayout",
      "options": {
        "spacing": "16px",
        "sx": {
          "padding": "16px"
        }
      },
      "elements": [
        {
          "type": "Typography",
          "label": "หน้าหลัก",
          "options": {
            "variant": "h6",
            "sx": {
              "padding": "4px 0"
            }
          }
        },
        {
          "type": "PaperLayout",
          "options": {
            "component": "section",
            "variant": "section"
          },
          "elements": [
            {
              "type": "StackLayout",
              "options": {
                "direction": "row"
              },
              "elements": [
                {
                  "type": "Typography",
                  "label": "Work List"
                }
              ]
            },
            {
              "type": "BoxLayout",
              "elements": [
                {
                  "type": "Datagrid",
                  "scope": "#",
                  "options": {
                    "grid": "CSENT/WorkList",
                    "dataContext": {
                      "query": "CSENT/WorkList",
                      "queryVariables": {
                        "assignee": "{user.login}"
                      }
                    }
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "PaperLayout",
          "options": {
            "component": "section",
            "variant": "section"
          },
          "elements": [
            {
              "type": "StackLayout",
              "options": {
                "direction": "row"
              },
              "elements": [
                {
                  "type": "Typography",
                  "label": "Work Basket"
                },
                {
                  "type": "ActionButton",
                  "label":  "Get Top Task",
                  "options": {
                    "operationType": "action",
                    "variant": "secondary",
                    "size": "small",
                    "validationSchema": false,
                    "conditionTypeId": "WorkBasket_GetTopTask",
                    "permission": "p_CSENT",
                    "payload": {
                      "routeUrl": "/csent/#[ID]/details",
                      "taskType": "{searchParams.stationName}"
                    }
                  }
                }
              ]
            },
            {
              "type": "StackLayout",
              "options": {
                "spacing": "16px"
              },
              "elements": [
                {
                  "type": "BoxLayout",
                  "options": {
                    "sx": {
                      "padding": "16px 16px 0 16px"
                    }
                  },
                  "elements": [
                    {
                      "type": "GridLayout",
                      "options": {
                        "container": "true",
                        "spacing": "16px"
                      },
                      "elements": [
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "TextField",
                              "label": "Application No.",
                              "scope": "#/properties/search/properties/application_number",
                              "options": {
                                "fullwidth": true,
                                "variant": "outlined"
                              },
                              "errors": {
                                "pattern": "กรุณาระบุข้อมูลเป็นตัวอักษรภาษาอังกฤษ ตัวเลข - และ _ เท่านั้น",
                                "minLength": "กรุณาระบุข้อมูลอย่างน้อย 5 ตัวอักษร"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Application Group",
                              "scope": "#/properties/search/properties/application_group",
                              "options": {
                                "fullwidth": true,
                                "autocomplete": true,
                                "variant": "outlined",
                                "enumDataPath": "application_group_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Application Type",
                              "scope": "#/properties/search/properties/application_type",
                              "options": {
                                "fullwidth": true,
                                "autocomplete": true,
                                "variant": "outlined",
                                "enumDataPath": "application_type_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Application Class",
                              "scope": "#/properties/search/properties/application_class",
                              "options": {
                                "fullwidth": true,
                                "autocomplete": true,
                                "variant": "outlined",
                                "enumDataPath": "application_class_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "ช่องทาง",
                              "scope": "#/properties/search/properties/application_channel",
                              "options": {
                                "fullwidth": true,
                                "autocomplete": true,
                                "variant": "outlined",
                                "enumDataPath": "application_channel_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "ชื่อลูกค้า",
                              "scope": "#/properties/search/properties/customer_name",
                              "options": {
                                "partialQuery": 5,
                                "fullwidth": true,
                                "autocomplete": true,
                                "variant": "outlined",
                                "query": "CSENT/CustomerNameSearch",
                                "converter": "autocompleteDataConverter",
                                "queryVariablesConverter": "inputToQueryVariables"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "Typography",
                              "label": "วันที่ได้รับเอกสาร",
                              "options": {
                                "variant": "label"
                              }
                            },
                            {
                              "type": "ExternalComponent",
                              "scope": "#/properties/search/properties/doc_in_one_datetime",
                              "options": {
                                "variant": "outlined",
                                "views": "year,month,day",
                                "externalComponentName": "thai-calendar",
                                "externalComponentScope": "remote1",
                                "externalComponentModule": "./ThaiDatePicker",
                                "minDate": "#/properties/thaiDateAfterSubtraction",
                                "maxDate": "#/properties/currentThaiDate",
                                "sx": {
                                  "width": "100%"
                                }
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "Typography",
                              "label": "วันที่ลงทะเบียน",
                              "options": {
                                "variant": "label"
                              }
                            },
                            {
                              "type": "ExternalComponent",
                              "scope": "#/properties/search/properties/registration_date",
                              "options": {
                                "variant": "outlined",
                                "views": "year,month,day",
                                "externalComponentName": "thai-calendar",
                                "externalComponentScope": "remote1",
                                "externalComponentModule": "./ThaiDatePicker",
                                "minDate": "#/properties/thaiDateAfterSubtraction",
                                "maxDate": "#/properties/currentThaiDate",
                                "sx": {
                                  "width": "100%"
                                }
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "StackLayout",
                  "options": {
                    "direction": "row",
                    "justifyContent": "space-between",
                    "sx": {
                      "padding": "0 16px"
                    }
                  },
                  "elements": [
                    {
                      "type": "ActionButton",
                      "label": "ล้างค่า",
                      "options": {
                        "variant": "secondaryError",
                        "operationType": "script",
                        "converter": "clear_work_basket_search",
                        "resultMappingSchema": {
                          "type": "object",
                          "properties": {
                            "search": {
                              "type": "object",
                              "properties": {
                                "application_group": {
                                  "type": "string"
                                },
                                "application_type": {
                                  "type": "string"
                                },
                                "application_class": {
                                  "type": "string"
                                },
                                "application_number": {
                                  "type": "string"
                                },
                                "application_channel": {
                                  "type": "string"
                                },
                                "customer_name": {
                                  "type": "string"
                                },
                                "registration_date": {
                                  "type": "object",
                                  "properties": {
                                    "from": {
                                      "type": "string"
                                    },
                                    "to": {
                                      "type": "string"
                                    }
                                  }
                                },
                                "doc_in_one_datetime": {
                                  "type": "object",
                                  "properties": {
                                    "from": {
                                      "type": "string"
                                    },
                                    "to": {
                                      "type": "string"
                                    }
                                  }
                                }
                              }
                            },
                            "workbasket_grid": {
                              "type": "object",
                              "properties": {
                                "filters": {
                                  "type": "array"
                                },
                                "pagination": {
                                  "type": "object",
                                  "properties": {
                                    "pageIndex": {
                                      "type": "number"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        },
                        "validationSchema": false
                      }
                    },
                    {
                      "type": "ActionButton",
                      "label": "ค้นหา",
                      "options": {
                        "variant": "primary",
                        "chain": [
                          {
                            "operationType": "script",
                            "converter": "check_work_basket_search_criteria",
                            "resultMappingSchema": {
                              "type": "object",
                              "properties": {
                                "canSubmit": {
                                  "type": "boolean",
                                  "dataPath": "canSubmit"
                                }
                              }
                            }
                          },
                          {
                            "if": {
                              "type": "object",
                              "required": [
                                "payload"
                              ],
                              "properties": {
                                "payload": {
                                  "type": "object",
                                  "required": [
                                    "data"
                                  ],
                                  "properties": {
                                    "data": {
                                      "required": [
                                        "canSubmit"
                                      ],
                                      "type": "object",
                                      "properties": {
                                        "canSubmit": {
                                          "type": "boolean",
                                          "const": true
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            },
                            "operationType": "script",
                            "converter": "work_basket_filter",
                            "resultMappingSchema": {
                              "type": "object",
                              "properties": {
                                "workbasket_grid": {
                                  "type": "object",
                                  "properties": {
                                    "filters": {
                                      "type": "array"
                                    },
                                    "pagination": {
                                      "type": "object",
                                      "properties": {
                                        "pageIndex": {
                                          "type": "number"
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          },
                          {
                            "if": {
                              "else": true
                            },
                            "$ref": "urn:zbo:uischema:basic_Error_EmptySearchFields"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "BoxLayout",
                  "options": {
                    "htmlID": "WorkBasket"
                  },
                  "elements": [
                    {
                      "type": "Datagrid",
                      "scope": "#/properties/workbasket_grid",
                      "options": {
                        "grid": "CSENT/WorkBasket",
                        "dataContext": {
                          "query": "CSENT/WorkBasket"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
