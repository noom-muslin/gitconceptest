                      }
                    },
                    "elements":  [
                      {
                        "type": "ActionButton",
                        "label": "ล้างค่า",
                        "options": {
                          "operationType": "script",
                          "converter": "clear_filter_applications_grid",
                          "variant": "secondaryError",
                          "resultMappingSchema": {
                            "type": "object",
                            "properties": {
                              "search_WL": {
                                "type": "object",
                                "properties": {
                                  "application_id": {
                                    "type": "string",
                                    "pattern": "^[A-Za-z0-9|_]+$"
                                  },
                                  "application_group": {
                                      "type": "string"
                                  },
                                  "product_type": {
                                      "type": "string"
                                  },
                                  "sub_product_type": {
                                      "type": "string"
                                  },
                                  "lane": {
                                      "type": "string"
                                  },
                                  "customer_firstname": {
                                      "type": "string"
                                  },
                                  "customer_lastname": {
                                      "type": "string"
                                  },
                                  "reference_type": {
                                      "type": "string"
                                  },
                                  "reference_no": {
                                      "type": "string",
                                      "pattern": "^[A-Za-z0-9|_]+$"
                                  }
                                }
                              },
                              "worklist_grid": {
                                "type": "object",
                                "properties": {
                                  "filters": {
                                    "type": "array"
                                  },
                                  "pagination": {
                                    "type": "object",
                                    "properties": {
                                      "pageIndex": {
                                        "type": "number"
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      },
                      {
                        "type": "ActionButton",
                        "label": "ค้นหา",
                        "options": {
                          "variant": "primary",
