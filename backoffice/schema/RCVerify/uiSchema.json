                              "operationType": "script",
                              "converter": "check_search_criteria_WL",
                              "resultMappingSchema": {
                                "type": "object",
                                "properties": {
                                  "canSubmit": {
                                    "type": "boolean",
                                    "dataPath": "canSubmit"
                                  }
                                }
                              }
                            },
                            {
                              "if": {
                                "type": "object",
                                "required": ["payload"],
                                "properties": {
                                  "payload": {
                                    "type": "object",
                                    "required": ["data"],
                                    "properties": {
                                      "data": {
                                        "required": ["canSubmit"],
                                        "type": "object",
                                        "properties": {
                                          "canSubmit": {
                                            "type": "boolean",
                                            "const": true
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              },
                              "operationType": "script",
                              "converter": "worklist_filter",
                              "size": "large",
                              "sx": {
                                "backgroundColor": "#5b21b6"
                              },
                              "resultMappingSchema": {
                                "type": "object",
                                "properties": {
                                  "worklist_grid": {
                                    "type": "object",
                                    "properties": {
                                      "filters": {
                                        "type": "array"
                                      },
                                      "pagination": {
                                        "type": "object",
                                        "properties": {
                                          "pageIndex": {
                                            "type": "number"
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            },
                            {
                              "if": {
                                "else": true
                              },
