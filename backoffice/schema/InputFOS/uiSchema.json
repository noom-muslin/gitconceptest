{
  "type": "BoxLayout",
  "elements": [
    {
      "type": "BoxLayout",
      "options": {
        "sx": {
          "padding": "16px",
          "backgroundColor": "primary.main"
        }
      },
      "elements": [
        {
          "type": "Typography",
          "label": "{zboRouteObject.label}",
          "options": {
            "variant": "h6White",
            "sx": {
              "padding": "4px 0"
            }
          }
        },
        {
          "type": "FormMapperListener",
          "options": {
            "resultMappingSchema": {
              "type": "object",
              "properties": {
                "modal": {
                  "converter": "inputFOSListenerStorage",
                  "type": "object",
                  "dataPath": []
                }
              }
            }
          }
        }
      ]
    },
    {
      "type": "StackLayout",
      "options": {
        "spacing": "16px",
        "sx": {
          "padding": "16px"
        }
      },
      "elements": [
        {
          "type": "Typography",
          "label": "หน้าหลัก",
          "options": {
            "variant": "h6",
            "sx": {
              "padding": "4px 0"
            }
          }
        },
        {
          "type": "PaperLayout",
          "options": {
            "component": "section",
            "variant": "section"
          },
          "elements": [
            {
              "type": "StackLayout",
              "options": {
                "direction": "row"
              },
              "elements": [
                {
                  "type": "Typography",
                  "label": "Work List"
                }
              ]
            },
            {
              "type": "BoxLayout",
              "elements": [
                {
                  "type": "Datagrid",
                  "scope": "#/properties/worklist_grid",
                  "options": {
                    "grid": "InputFOS/WorkList",
                    "dataContext": {
                      "query": "InputFOS/WorkList",
                      "queryVariables": {
                        "task_type_id": "InputFOS",
                        "performing_by": "{user.login}",
                        "status_id": "InProgress"
                      }
                    }
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "PaperLayout",
          "options": {
            "component": "section",
            "variant": "section"
          },
          "elements": [
            {
              "type": "StackLayout",
              "options": {
                "direction": "row"
              },
              "elements": [
                {
                  "type": "Typography",
                  "label": "Work Basket"
                },
                {
                  "type": "ActionButton",
                  "label": "Get Top Task",
                  "options": {
                    "operationType": "script",
                    "converter": "workbasket_get_top_task",
                    "variant": "secondary",
                    "size": "small"
                  }
                }
              ]
            },
            {
              "type": "StackLayout",
              "options": {
                "spacing": "16px"
              },
              "elements": [
                {
                  "type": "BoxLayout",
                  "elements": [
                    {
                      "type": "Group",
                      "options": {
                        "sx": {
                          "height": "0px"
                        }
                      },
                      "rule": {
                        "effect": "SHOW",
                        "condition": {
                          "scope": "#/properties/modal/properties/showNotification",
                          "schema": {
                            "const": "INPUT_FOS_SAVE_DRAFT"
                          }
                        }
                      },
                      "elements": [
                        {
                          "type": "Notification",
                          "label": "บันทึกแบบร่างแล้ว",
                          "options": {
                            "preventDuplicate": 1,
                            "autoHideDuration": 3000,
                            "persist": false,
                            "variant": "success",
                            "hideCloseButton": true,
                            "anchorOrigin": {
                              "horizontal": "center",
                              "vertical": "top",
                              "style": {
                                "color": "#166534",
                                "display": "flex",
                                "justifyContent": "center",
                                "backgroundColor": "#F0FDF4",
                                "min-width": "auto",
                                "width": "200px",
                                "height": "44px",
                                "margin-left": "63.5px"
                              }
                            }
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "BoxLayout",
                  "options": {
                    "sx": {
                      "padding": "16px 16px 0 16px"
                    }
                  },
                  "elements": [
                    {
                      "type": "GridLayout",
                      "options": {
                        "container": "true",
                        "spacing": "16px"
                      },
                      "elements": [
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "Typography",
                              "label": "วันที่เอกสารถึง WB",
                              "options": {
                                "variant": "label"
                              }
                            },
                            {
                              "type": "ExternalComponent",
                              "scope": "#/properties/search/properties/doc_in_datetime",
                              "options": {
                                "externalComponentName": "thai-calendar",
                                "externalComponentScope": "remote1",
                                "externalComponentModule": "./ThaiDateRange",
                                "views": "year,month,day",
                                "variant": "outlined",
                                "sx": {
                                  "width": "100%"
                                }
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "TextField",
                              "label": "Application No.",
                              "scope": "#/properties/search/properties/application_number",
                              "errors": {
                                "pattern": "กรุณาระบุข้อมูลเป็นตัวอักษรภาษาอังกฤษ ตัวเลข - และ _ เท่านั้น",
                                "minLength": "กรุณาระบุข้อมูลอย่างน้อย 5 ตัวอักษร"
                              },
                              "options": {
                                "fullwidth": true,
                                "variant": "outlined"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Application Group",
                              "scope": "#/properties/search/properties/application_group",
                              "options": {
                                "variant": "outlined",
                                "fullwidth": true,
                                "autocomplete": true,
                                "enumDataPath": "application_group_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Application Class",
                              "scope": "#/properties/search/properties/application_class",
                              "options": {
                                "variant": "outlined",
                                "fullwidth": true,
                                "autocomplete": true,
                                "enumDataPath": "application_class_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Product Group",
                              "scope": "#/properties/search/properties/product_group",
                              "options": {
                                "variant": "outlined",
                                "fullwidth": true,
                                "autocomplete": true,
                                "enumDataPath": "product_group_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "SelectField",
                              "label": "Product Type",
                              "scope": "#/properties/search/properties/product_type",
                              "options": {
                                "variant": "outlined",
                                "fullwidth": true,
                                "autocomplete": true,
                                "enumDataPath": "product_type_enum"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "TextField",
                              "label": "ชื่อลูกค้า",
                              "scope": "#/properties/search/properties/customer_name",
                              "errors": {
                                "pattern": "Special characters are not allowed."
                              },
                              "options": {
                                "fullwidth": true,
                                "variant": "outlined"
                              }
                            }
                          ]
                        },
                        {
                          "type": "GridLayout",
                          "options": {
                            "xs": "4"
                          },
                          "elements": [
                            {
                              "type": "TextField",
                              "label": "นามสกุลลูกค้า",
                              "scope": "#/properties/search/properties/customer_lastname",
                              "errors": {
                                "pattern": "Special characters are not allowed."
                              },
                              "options": {
                                "fullwidth": true,
                                "variant": "outlined"
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "StackLayout",
                  "options": {
                    "direction": "row",
                    "justifyContent": "space-between",
                    "sx": {
                      "padding": "0 16px"
                    }
                  },
                  "elements": [
                    {
                      "type": "ActionButton",
                      "label": "ล้างค่า",
                      "options": {
                        "variant": "secondaryError",
                        "operationType": "script",
                        "converter": "clear_work_basket_search",
                        "validationSchema": false,
                        "resultMappingSchema": {
                          "type": "object",
                          "properties": {
                            "search": {
                              "type": "object",
                              "properties": {
                                "application_number": {
                                  "type": "string",
                                  "pattern": "^[A-Za-z0-9_-]+$"
                                },
                                "application_group": {
                                  "type": "string"
                                },
                                "application_class": {
                                  "type": "string"
                                },
                                "product_group": {
                                  "type": "string"
                                },
                                "product_type": {
                                  "type": "string"
                                },
                                "customer_name": {
                                  "type": "string",
                                  "pattern": "^[A-Za-z0-9ก-๙]+$"
                                },
                                "customer_lastname": {
                                  "type": "string",
                                  "pattern": "^[A-Za-z0-9ก-๙]+$"
                                },
                                "doc_in_datetime": {
                                  "type": "object",
                                  "properties": {
                                    "from": {
                                      "type": "string"
                                    },
                                    "to": {
                                      "type": "string"
                                    }
                                  }
                                }
                              }
                            },
                            "workbasket_grid": {
                              "type": "object",
                              "properties": {
                                "filters": {
                                  "type": "array"
                                },
                                "pagination": {
                                  "type": "object",
                                  "properties": {
                                    "pageIndex": {
                                      "type": "number"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    },
                    {
                      "type": "ActionButton",
                      "label": "ค้นหา",
                      "options": {
                        "variant": "primary",
                        "chain": [
                          {
                            "operationType": "script",
                            "converter": "check_work_basket_search_criteria",
                            "resultMappingSchema": {
                              "type": "object",
                              "properties": {
                                "canSubmit": {
                                  "type": "boolean",
                                  "dataPath": "canSubmit"
                                }
                              }
                            }
                          },
                          {
                            "if": {
                              "type": "object",
                              "required": ["payload"],
                              "properties": {
                                "payload": {
                                  "type": "object",
                                  "required": ["data"],
                                  "properties": {
                                    "data": {
                                      "required": ["canSubmit"],
                                      "type": "object",
                                      "properties": {
                                        "canSubmit": {
                                          "type": "boolean",
                                          "const": true
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            },
                            "operationType": "script",
                            "converter": "work_basket_filter",
                            "resultMappingSchema": {
                              "type": "object",
                              "properties": {
                                "workbasket_grid": {
                                  "type": "object",
                                  "properties": {
                                    "filters": {
                                      "type": "array"
                                    },
                                    "pagination": {
                                      "type": "object",
                                      "properties": {
                                        "pageIndex": {
                                          "type": "number"
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          },
                          {
                            "if": {
                              "else": true
                            },
                            "$ref": "urn:zbo:uischema:basic_Error_EmptySearchFields"
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "BoxLayout",
                  "options": {
                    "htmlID": "WorkBasket"
                  },
                  "elements": [
                    {
                      "type": "Datagrid",
                      "scope": "#/properties/workbasket_grid",
                      "options": {
                        "grid": "InputFOS/WorkBasket",
                        "dataContext": {
                          "query": "InputFOS/WorkBasket",
                          "queryVariables": {
                            "taskStatus": "InputFOS"
                          }
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
